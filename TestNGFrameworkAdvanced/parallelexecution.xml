<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!-- Running Parallel Tests through XML file-->
<suite name="Parallel Execution Test"  parallel="tests">

	<test name="Test BrowserName1">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="parallelexecution.ParallelExecutionThroughXML"></class>
		</classes>
	</test>

	<test name="Test BrowserName2">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="parallelexecution.ParallelExecutionThroughXML"></class>
		</classes>
	</test>

</suite> 
