package impprograms;

import java.util.Scanner;

public class OneToHundredNumbersWithoutLoop {

	public static void getNumbers(int maxNumber) {

		int num = 1;
		int sum=0;

		if (sum < maxNumber) {

			sum = sum + num;
			System.out.println("Total numbers are : "+sum);
		}
	}

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Please specify the max number");
		int maxNumber = sc.nextInt();
		sc.close();
		getNumbers(maxNumber);

	}
}
