package testpackage3;

import java.util.ArrayList;

public class GreatestNumberFromList {

	public static void main(String[] args) {
		
		ArrayList<Integer> list = new ArrayList<Integer>();
		list.add(10);
		list.add(20);
		list.add(50);
		list.add(40);
		list.add(30);
		
		int maxNumber = list.get(0);
		
		for(Integer c : list) {
			
			if(c>maxNumber) {
				
				maxNumber = c;
			}
		}
		

	}
}
