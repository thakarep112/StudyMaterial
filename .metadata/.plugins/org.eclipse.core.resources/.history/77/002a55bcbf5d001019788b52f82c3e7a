package testpackage2;

public class TestClass {
	
	int num = 10;

	public static void main(String[] args) {
		
		TestClass tc = new TestClass();
		tc.getNumber();
		System.out.println(tc.num);
	}
	
	public void getNumber() {
		
		int num=0;
		
		this.num = num;
		
		System.out.println(num);
		
	}
}
